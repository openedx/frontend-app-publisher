{
  "extends": [
    "config:base",
    ":automergeLinters",
    ":automergeTesters",
    ":automergeMinor",
    ":noUnscheduledUpdates",
    ":semanticCommits"
  ],
  "rebaseStalePrs": true,
  "schedule": [
    "every weekday"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["node", "npm"],
      "enabled": false
    }
  ],
  "constraints": {
    "@tinymce/tinymce-react": "3.8.4"
  }
}
