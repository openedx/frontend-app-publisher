{
  "name": "edx-publisher-frontend",
  "version": "0.1.0",
  "description": "Frontend for the Publisher Application",
  "repository": "https://github.com/edx/publisher-frontend",
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production webpack --config=config/webpack.prod.config.js",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "prepush": "npm run lint",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=development BABEL_ENV=development node_modules/.bin/webpack-dev-server --config=config/webpack.dev.config.js --progress",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage",
    "test": "NODE_ENV=test jest --coverage --runInBand",
    "snapshot": "jest --updateSnapshot"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@edx/edx-bootstrap": "^1.0.3",
    "@edx/frontend-auth": "^3.0.1",
    "@edx/frontend-component-footer": "^1.0.0",
    "@edx/paragon": "^3.9.3",
    "@tinymce/tinymce-react": "^2.5.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "connected-react-router": "^5.0.0",
    "email-prop-type": "^3.0.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "moment": "^2.24.0",
    "prop-types": "^15.5.10",
    "query-string": "^5.1.1",
    "react": "^16.4.2",
    "react-beautiful-dnd": "^10.1.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "tinymce": "^5.0.0",
    "validator": "^10.9.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^2.0.0",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "es-check": "^5.0.0",
    "eslint-config-edx": "^4.0.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.7.2",
    "react-dev-utils": "^7.0.1",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.2.1"
  },
  "greenkeeper": {
    "ignore": [
      "babel-loader"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@edx/paragon)/).*/"
    ],
    "globals": {
      "courseContext": {}
    }
  }
}
