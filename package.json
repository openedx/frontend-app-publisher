{
  "name": "edx-frontend-app-publisher",
  "version": "0.1.0",
  "description": "Frontend for the Publisher Application",
  "repository": "https://github.com/edx/frontend-app-publisher",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production webpack --config=config/webpack.prod.config.js",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "prepush": "npm run lint; npm run stylelint",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=development BABEL_ENV=development node_modules/.bin/webpack-dev-server --config=config/webpack.dev.config.js --progress",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage",
    "test": "TZ=GMT NODE_ENV=test jest --coverage --runInBand",
    "snapshot": "TZ=GMT jest --updateSnapshot",
    "stylelint": "stylelint '**/*.scss'"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@edx/frontend-auth": "5.3.6",
    "@edx/frontend-component-footer": "4.1.5",
    "@edx/frontend-logging": "3.0.1",
    "@edx/paragon": "7.2.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "@tinymce/tinymce-react": "2.6.1",
    "classnames": "2.2.6",
    "core-js": "3.6.4",
    "connected-react-router": "5.0.1",
    "email-prop-type": "3.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fast-json-stable-stringify": "2.0.0",
    "font-awesome": "4.7.0",
    "history": "4.10.1",
    "html-webpack-new-relic-plugin": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "jsx-to-string": "1.4.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "new-relic-source-map-webpack-plugin": "1.2.0",
    "prop-types": "15.7.2",
    "query-string": "5.1.1",
    "react": "16.12.0",
    "react-autosuggest": "9.4.3",
    "react-beautiful-dnd": "10.1.1",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-intl": "2.9.0",
    "react-redux": "5.1.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-select": "3.0.8",
    "react-tooltip": "3.11.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-form": "7.4.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.3",
    "stylelint-declaration-use-variable": "1.7.0",
    "stylelint-scss": "3.11.1",
    "tinymce": "5.1.4",
    "tinymce-language-selector": "edx/tinymce-language-selector",
    "validator": "10.11.0",
    "webpack": "4.41.2",
    "webpack-merge": "4.2.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@edx/eslint-config": "1.1.4",
    "@edx/stylelint-config-edx": "1.1.1",
    "axios-mock-adapter": "1.17.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "codecov": "3.6.1",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "2.1.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "es-check": "5.0.0",
    "file-loader": "3.0.1",
    "gh-pages": "2.1.1",
    "html-webpack-harddisk-plugin": "1.0.1",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "4.6.0",
    "jest": "25.1.0",
    "node-sass": "4.13.0",
    "react-dev-utils": "7.0.5",
    "react-test-renderer": "16.12.0",
    "redux-mock-store": "1.5.3",
    "sass-loader": "7.3.1",
    "style-loader": "0.23.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.9.0"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(tinymce-language-selector|@edx/paragon)/).*/"
    ],
    "globals": {
      "courseContext": {}
    }
  }
}
